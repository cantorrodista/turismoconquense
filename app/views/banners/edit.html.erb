<div class="form_toolbar">
	<ul>
		<li>
			<%= link_to 'Volver', banners_path, :class => "back"%> 
		</li>
			<li>
				<%= link_to 'Eliminar', banner_path(@banner),:confirm => "Estás a punto de eliminar '#{@banner.title}', ¿estás seguro?", :method => :delete, :class => "remove" %>
			</li>
		
	</ul>
</div>
<% form_for @banner, :url => banner_path,
           :html => { :multipart => true} do |f| %>
        <%= render :partial => 'form', :locals => {:banner => @banner, :f => f} %>
<% end %>